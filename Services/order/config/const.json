    "fulFillmentStatus": {
        "IBT_M": {
            "draft": "!submittedQty  && !confirmedQty && !shippedQty && !receivedQty",
            "submitted": "submittedQty  && !confirmedQty && !shippedQty",
            "confirmed": "((submittedQty  && confirmedQty) || rejectedQty) && !shippedQty",
            "partiallyShipped": "(shippedQty < confirmedQty)",
            "rejected": "(confirmedQty == 0) || (confirmedQty == '') && (rejectedQty == submittedQty)",
            "shippedFull": "((shippedQty > 0) && (shippedQty == confirmedQty)) || ((shippedQty == confirmedQty) && (receivedQty < shippedQty))",
            "receivedFull": "((confirmedQty == shippedQty == receivedQty) && (shippedQty > 0)) || ((confirmedQty == shippedQty) && (receivedQty >= shippedQty) && (shippedQty > 0)) || (!submittedQty  && !confirmedQty && !shippedQty && receivedQty) "
        },
        "PUSH": {
            "draft": "!submittedQty  && !confirmedQty && !shippedQty",
            "shipped": "shippedQty && !receivedQty",
            "received": "receivedQty"
        },
        "receivedWithDescrepancies": {
            "IBT_M": {
                "shippedFull": "((shippedQty > 0) && (shippedQty == confirmedQty)) || ((shippedQty == confirmedQty) && (receivedQty < shippedQty))",
                "receivedFull": "((confirmedQty == shippedQty == receivedQty) && (shippedQty > 0)) || ((confirmedQty == shippedQty) && (receivedQty >= shippedQty) && (shippedQty > 0)) || (!submittedQty  && !confirmedQty && !shippedQty && receivedQty) "
            },
            "PUSH": {
                "shipped": "shippedQty && !receivedQty",
                "received": "(receivedQty && shippedQty && (receivedQty != shippedQty))"
            }
        }
    },
